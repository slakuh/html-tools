<?xml version="1.0"?>
<dwquery>
  <queryparams matchcase="false" ignorewhitespace="false" useregexp="true" wholeword="false" />
  <find>
    <qtext qname="(?:&lt;/p&gt;\s+&lt;div&gt;)(.*?)(?:-)" qraw="true"></qtext>
  </find>
  <replace action="replaceText" param1="</p><div><strong>$1</strong> -" param2=""/>
</dwquery>
